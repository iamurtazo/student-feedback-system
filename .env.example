# Environment Variables for Student Feedback System
# Rename this file to .env and update the values

# Django Secret Key (keep this secret in production!)
SECRET_KEY=your_secret_key_here

# Debug mode (set to False in production)
DEBUG=True

# Allowed hosts (comma-separated list)
ALLOWED_HOSTS=127.0.0.1,localhost

# Database configuration (uncomment and modify for production)
# DB_ENGINE=django.db.backends.postgresql
# DB_NAME=student_feedback
# DB_USER=dbuser
# DB_PASSWORD=dbpassword
# DB_HOST=localhost
# DB_PORT=5432
